@model IEnumerable<tjadmin.Models.tbl_TeachingJob>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            jTitle_en
        </th>
        <th>
            Organization
        </th>
        <th>
            jClosingDate
        </th>
        <th>
            jUploadDate
        </th>
        @*<th>
            jFeaturedJob
        </th>*@
        <th>
            jViewCount
        </th>
        <th>
            jExecutiveJob
        </th>
        <th>
            jActive
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.jTitle_en)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.aspnet_Users.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jClosingDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jUploadDate)
        </td>
@*        <td>
            @Html.DisplayFor(modelItem => item.jFeaturedJob)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.jViewCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jExecutiveJob)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jActive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.jId }) |
            @Html.ActionLink("Details", "Details", new { id=item.jId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.jId })
        </td>
    </tr>
}

</table>
