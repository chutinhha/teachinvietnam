<?xml version="1.0"?>
<configuration>

  <location path="Register.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>
  <location path="ChangePassword.aspx">
    <system.web>
      <authorization>
        <allow roles="employee, Organization"/>
        <deny users="*"/>
      </authorization>
    </system.web>
  </location>
  <location path="my-jobs.aspx">
    <system.web>
      <authorization>
        <allow roles="employee"/>
        <deny users="*"/>
      </authorization>
    </system.web>
  </location>
  <location path="myAccount.aspx">
    <system.web>
      <authorization>
        <allow roles="employee"/>
        <deny users="*"/>
        <!--<deny users="?"/>
        <allow users="*"/>-->
      </authorization>
    </system.web>
  </location>
  <location path="RecruiterInfo.aspx">
    <system.web>
      <authorization>
        <allow roles="organization"/>
        <deny users="*"/>
        <!--<deny users="?"/>
        <allow users="*"/>-->
      </authorization>
    </system.web>
  </location>
  <!--<system.web>
    <authorization>
      <deny users="?"/>
    </authorization>
  </system.web>-->

</configuration>
